	<queryString>
		<![CDATA[SELECT
     facturaventa.`idfactura` AS facturaventa_idfactura,
     facturaventa.`fac_fecha` AS facturaventa_fac_fecha,
     facturaventa.`fac_condicion` AS facturaventa_fac_condicion,
     facturaventa.`fac_estado` AS facturaventa_fac_estado,
     facturaventa.`idpersonal` AS facturaventa_idpersonal,
     facturaventa.`idcliente` AS facturaventa_idcliente,
     facturaventa.`fac_nro` AS facturaventa_fac_nro,
     cliente.`idcliente` AS cliente_idcliente,
     cliente.`cli_nombre` AS cliente_cli_nombre,
     cliente.`cli_apellido` AS cliente_cli_apellido,
concat(cliente.`cli_nombre`,' ',cliente.`cli_apellido`) as nombrecliente,
     cliente.`cli_ci` AS cliente_cli_ci,
if(facturaventa.`fac_condicion`='CONTADO','X',' ')as contado,
if(facturaventa.`fac_condicion`='CREDITO','X',' ')as credito

FROM
     `cliente` cliente inner join `facturaventa` facturaventa on cliente.`idcliente` = facturaventa.`idcliente`

WHERE facturaventa.`idfactura`=$P{factura}]]>
	</queryString>
