	<queryString>
		<![CDATA[SELECT
     detallefactura.`idfactura` AS detallefactura_idfactura,
     detallefactura.`idproducto` AS detallefactura_idproducto,
     detallefactura.`fac_cantidad` AS detallefactura_fac_cantidad,
     detallefactura.`fec_precio` AS detallefactura_fec_precio,
     producto.`idproducto` AS producto_idproducto,
     producto.`pro_nombre` AS producto_pro_nombre,
     producto.`pro_precio` AS producto_pro_precio,
     producto.`pro_cantidad` AS producto_pro_cantidad,
     producto.`pro_iva` AS producto_pro_iva,
     producto.`idproveedor` AS producto_idproveedor,
if(producto.`pro_iva`='0',detallefactura.`fac_cantidad`*detallefactura.`fec_precio`,' ') as excenta,
if(producto.`pro_iva`='5',detallefactura.`fac_cantidad`*detallefactura.`fec_precio`,' ') as cinco,
if(producto.`pro_iva`='10',detallefactura.`fac_cantidad`*detallefactura.`fec_precio`,' ') as diez
FROM
     `producto` producto inner join `detallefactura` detallefactura on producto.`idproducto` =
     detallefactura.`idproducto`
     where detallefactura.`idfactura`=$P{factura}]]>
	</queryString>
